{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/demo.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/blog.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/demo",
      "dest": "/api/demo.js"
    },
    {
      "src": "/blog",
      "dest": "/api/blog.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}